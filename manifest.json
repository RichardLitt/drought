{
  "name": "Drought",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "https://*.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*"],
      "js": ["build.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "web_accessible_resources": [
    "images/*"
  ]
}
